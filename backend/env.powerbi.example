# PowerBI Configuration
# Copy this file to .env and fill in your PowerBI service principal details

# PowerBI Service Principal Configuration
POWERBI_CLIENT_ID=your_service_principal_client_id
POWERBI_CLIENT_SECRET=your_service_principal_client_secret
POWERBI_TENANT_ID=your_azure_tenant_id

# PowerBI Workspace Configuration
POWERBI_WORKSPACE_ID=your_powerbi_workspace_id

# Optional: PowerBI Bot Configuration (for notifications)
POWERBI_BOT_USERNAME=your_powerbi_bot_username
POWERBI_BOT_TOKEN=your_powerbi_bot_token

# PowerBI API Configuration
POWERBI_API_BASE_URL=https://api.powerbi.com/v1.0/myorg
POWERBI_EMBED_BASE_URL=https://app.powerbi.com

# PowerBI Embed Token Configuration
POWERBI_EMBED_TOKEN_EXPIRY_MINUTES=60
POWERBI_DEFAULT_ACCESS_LEVEL=View

# PowerBI Dataset Configuration
POWERBI_DEFAULT_DATASET_ID=your_default_dataset_id
POWERBI_DEFAULT_REPORT_ID=your_default_report_id
POWERBI_DEFAULT_DASHBOARD_ID=your_default_dashboard_id

# PowerBI Security Configuration
POWERBI_ALLOW_SAVE_AS=false
POWERBI_ALLOW_PRINT=false
POWERBI_ALLOW_FILTER=false
POWERBI_ALLOW_DOWNLOAD=false

# PowerBI Logging Configuration
POWERBI_ENABLE_LOGGING=true
POWERBI_LOG_LEVEL=info

# PowerBI Rate Limiting
POWERBI_RATE_LIMIT_REQUESTS_PER_MINUTE=60
POWERBI_RATE_LIMIT_BURST=10
